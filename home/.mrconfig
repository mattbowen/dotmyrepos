[.homesick/repos/homeshick]
order = 0
checkout = git clone 'git://github.com/andsens/homeshick.git' 'homeshick'

[.local/zgen]
checkout = git clone 'git@github.com:tarjoilija/zgen.git' 'zgen'

[.homesick/repos/dotzsh]
checkout = ~/.homesick/repos/homeshick/bin/homeshick clone 'git@github.com:mattbowen/dotzsh.git'

[.homesick/repos/dotvim]
checkout = ~/.homesick/repos/homeshick/bin/homeshick clone 'git@github.com:mattbowen/dotvim.git' && ~/.homesick/repos/dotvim/home/.vim/setup.sh

[.homesick/repos/dotmyrepos]
checkout = ~/.homesick/repos/homeshick/bin/homeshick clone 'git@github.com:mattbowen/dotmyrepos.git'
